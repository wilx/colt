package cern.colt;

/**
 * Information about the current release.
 * Use this class to distinguish releases and base runtime decisions upon.
 */
public final class Version {
/**
 * Not yet commented.
 */
private Version() {
}
/**
 * Returns the major version of this release; for example version 1.2.3 returns 1.
 */
public static int getMajorVersion() {
	return 1;
}
/**
 * Returns the micro version of this release; for example version 1.2.3 returns 3.
 */
public static int getMicroVersion() {
	return 0;
}
/**
 * Returns the minor version of this release; for example version 1.2.3 returns 2.
 */
public static int getMinorVersion() {
	return 0;
}
private static final String version = "1.0.0.8";\n
}
